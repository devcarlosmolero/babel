{
  "name": "babel",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "npm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "migrations:gen": "drizzle-kit generate",
    "migrations:up": "drizzle-kit up:sqlite",
    "local:migrations:apply": "wrangler d1 migrations apply babel-db --local",
    "local:d1:init": "npm run local:migrations:apply --local && wrangler d1 execute babel-db --file='./fixtures/seed.development.sql'",
    "local:rm": "rm -rf ./.wrangler/state/v3/d1/*",
    "migrations:apply": "wrangler d1 migrations apply babel-db",
    "preview": "npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "git:precommit": "node scripts/remove-dev-dependencies.js && git add package.json || true",
    "git:postcommit": "node scripts/restore-dev-dependencies.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-switch": "^1.2.2",
    "@remix-run/cloudflare": "^2.9.2",
    "@remix-run/cloudflare-pages": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@tsndr/cloudflare-worker-jwt": "^2.5.3",
    "bcrypt-edge": "^0.1.1",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.40.0",
    "isbot": "^4.1.0",
    "lucide-react": "^0.418.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^10.0.5",
    "remix-flat-routes": "^0.8.5",
    "remix-utils": "^8.5.0",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240620.0",
    "@remix-run/dev": "^2.9.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.13",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.30.5",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.1.7",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwind-hamburgers": "^1.3.5",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.19.3",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "3.57.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}